Cell[CellGroupData[{

Cell[BoxData[
    \(\(\(\n\)\(Gradf1[x_] := 
      Transpose[{Df1[x, {1, 0, 0}], Df1[x, {0, 1, 0}], Df1[x, {0, 0, 1}]}]\n
    Gradf2[x_] := 
      Transpose[{Df2[x, {1, 0, 0}], Df2[x, {0, 1, 0}], Df2[x, {0, 0, 1}]}]\n
    Gradf3[x_] := 
      Transpose[{Df3[x, {1, 0, 0}], Df3[x, {0, 1, 0}], 
          Df3[x, {0, 0, 1}]}]\[IndentingNewLine] (*Gradf1[t_] := 
        Transpose[{D[f1[xvec], x], D[f1[xvec], y], 
              D[f1[xvec], y]}] /. {x \[Rule] t[\([1]\)], 
            y \[Rule] t[\([2]\)], z \[Rule] t[\([3]\)]}*) \n
    Gradf1[xvec]\n
    Gradf2[xvec]\n
    Gradf3[xvec]\n
    Gradf1[rvec] . uvec - Df1[rvec, uvec]\n
    Gradf2[rvec] . uvec - Df2[rvec, uvec]\n
    Gradf3[rvec] . uvec - Df3[rvec, uvec]\)\)\)], "Input"],

Cell[BoxData[
    \({{\(-1\), 0, 0}, {0, \(-1\), 0}, {0, 0, 1}}\)], "Output"],

