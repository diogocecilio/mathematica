Cell[BoxData[{
    \(ComputeU[F_] := 
      Block[{FtF, EigVecFtF, EigValFtF}, \n\t\tFtF = 
          N[Transpose[F] . F]; \n\t\tEigVecFtF = 
          Eigenvectors[FtF]; \n\t\tEigValFtF = 
          DiagonalMatrix[Eigenvalues[FtF]]; \n\t\tEigVecFtF . \@EigValFtF . 
          EigVecFtF\n\t\t]\), "\n", 
    \(ComputeR[F_] := N[F] . Inverse[ComputeU[F]]\), "\n", 
    \(F = Gradf3[{1. , 1. , 1. }]\  /. \ R -> 20. \), "\n", 
    \(ComputeU[F]\), "\n", 
    \(ComputeR[F]\)}], "Input"],

